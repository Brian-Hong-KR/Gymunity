<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.gymunity.board.repository.MemRepository">

  <insert id="savemem" parameterType="MemDTO">
	INSERT INTO members
	(mem_user_id, mem_grade_id, mem_ch_id)
  	VALUES (#{mem_user_id}, #{mem_grade_id}, #{mem_ch_id})
  </insert>
  

</mapper>   


















 